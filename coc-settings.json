{
	"tsserver.log": "verbose",
	"tsserver.experimental.enableProjectDiagnostics": false,
	"tsserver.useLocalTsdk": true,
	"prettier.tabWidth": 4,
	"php-cs-fixer.activateTool": "pint",
	"[php]": {
		"coc.preferences.formatOnSave": true
	},
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod"],
			"filetypes": ["go"]
		}
	}
}
